<h1>Movies#index</h1>
<p>Find me in app/views/movies/index.html.erb</p>


<table>
<thead>
<tr>
<th>ID</th>
<th>映画タイトル</th>
<th>公開年</th>
<th>上映中か</th>
<th>概要</th>
<th>画像URL</th>
<th>作成日時</th>
<th>更新日時</th>
</tr>
</thead>
<tbody>

<% @movies.each do |movie| %>
<tr>
    <td><%= movie.id %></td>
    <td><%= movie.name %></td>
    <td><%= movie.year %></td>
    <% if movie.is_showing then %>
        <td>上映中</td>
    <% else %>
        <td>上映予定</td>
    <% end %>
    <td><%= movie.image_url %></td>
    <td><%= movie.created_at %></td>
    <td><%= movie.updated_at %></td>
</tr>
<% end %>

</tbody>
</table>
